# OAuth2 설정 - 실제 발급받은 키로 교체하세요
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

KAKAO_CLIENT_ID=your_kakao_client_id_here
KAKAO_CLIENT_SECRET=your_kakao_client_secret_here

NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# Database 설정
DB_URL=jdbc:mysql://localhost:3306/iovu?serverTimezone=Asia/Seoul&characterEncoding=UTF-8
DB_USERNAME=root
DB_PASSWORD=your_database_password_here

# JWT 설정 - 보안을 위해 복잡한 키로 변경하세요 (최소 256비트)
JWT_SECRET=your_super_secure_jwt_secret_key_here_must_be_at_least_256_bits_long
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000

# CORS 설정
CORS_ALLOWED_ORIGINS=http://localhost:3000

# Server 설정
SERVER_PORT=8081

# 로깅 레벨 (개발: DEBUG, 운영: INFO)
LOG_LEVEL_SECURITY=DEBUG
LOG_LEVEL_OAUTH2=DEBUG
